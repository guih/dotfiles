[user]
	name = guih
	email = guih.matsumoto@gmail.com
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
 	ui = true
[core]
	excludesfile = /opt/boxen/config/git/gitignore
	editor = vim
	autocrlf = input
[apply]
	whitespace = nowarn
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[github]
	user = guih
[credential]
	helper = /opt/boxen/bin/boxen-git-credential
[push]
  default = simple
[pull]
  default = simple

[hub]
protocol = https

